{
    "manifest_version": 3,
    "name": "CF Submition Searcher",
    "version": "1.2.0",
    "description": "Tracks your Codeforces submissions and shows your daily activity in a simple view.",
    "icons": {
        "414": "CFSS.png"
    },
    "action": {
        "default_title": "Open My Extension",
        "default_popup": "popup.html",
        "default_icon": {
            "414": "CFSS.png"
        }
    },
    "permissions": [
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "https://codeforces.com/*"
    ]
}